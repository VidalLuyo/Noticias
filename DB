USE master;
DROP DATABASE Noticias;
CREATE DATABASE Noticias;
USE Noticias;

drop table Contenido;
CREATE TABLE Contenido(
	id int identity(1,1) primary key,
	Contenido varchar(250),
	descripcion varchar(250),
	Img varchar(259)
);


